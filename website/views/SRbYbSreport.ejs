<%- include("partials/header"); -%>
<h1>Store Revenue by Year by State &#160 <%= currentState %></h1>

<form id="tableForm" action="/SRbYbSreport" method="post">
    <select class="selectpicker" data-style="btn-info" name="selectpicker">
        <optgroup label="Select State">
            <option > Select State </option>
            <% SRRstate.forEach(function(e) { %>
            <option name=<%= e.state %> value=<%= e.state %>> <%= e.state %> </option>
            <% }) %>
        </optgroup>
    </select>
    <input type="submit" value = "Select State" />
</form>


<table>
  <tr>
    <th>Store Number</th>  <th>Store Address</th>  <th>City Name</th>  <th>Sales Year</th>  <th>Total Revenue</th>
  </tr>
  <% SRR.forEach(function(e) { %>
   <tr>
      <td> <%= e.store_number %> </td>
      <td> <%= e.store_address %> </td>
      <td> <%= e.city %> </td>
      <td> <%= e.sales_year %> </td>
      <td> <%= e.Total_Revenue.toFixed(0) %> </td>
    </tr>
  <% }) %>
</table>

<%- include("partials/footer"); -%>
